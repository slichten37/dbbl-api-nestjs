model Bowler {
  id           String   @id @default(uuid())
  name         String
  createdAtUtc DateTime @default(now())
  updatedAtUtc DateTime @updatedAt

  teams  Team[]
  frames Frame[]

  originalSubstitutions   MatchSubstitution[] @relation("SubOriginal")
  substituteSubstitutions MatchSubstitution[] @relation("SubSubstitute")
}
